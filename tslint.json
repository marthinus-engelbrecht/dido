{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ],
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "array-type": [true, "array"],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-reference": true,
    "no-parameter-reassignment": true,
    "only-arrow-functions": false
  },
  "rulesDirectory": []
}